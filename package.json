{
  "name": "land-registry-elements",
  "config": {
    "port": "3000"
  },
  "version": "0.0.1",
  "description": "Land registry frontend pattern library - building on top of http://govuk-elements.herokuapp.com",
  "main": "build/build.js",
  "scripts": {
    "test": "npm run build && npm run generateTestURLs && mocha && npm run eslint",
    "eslint": "eslint src/** --ignore-pattern build.js --ignore-pattern src/elements/land-registry/polyfills",
    "generateTestURLs": "node build/tasks/testURLs.js",
    "build": "node build/doBuild.js",
    "build-prod": "node build/doBuildProd.js",
    "start": "node build/devServer.js",
    "dev": "supervisor -i test,dist,.tmp,node_modules -e js,scss,hogan,yaml build/devServer.js",
    "zipTestRenderings": "node build/zipTestRenderings.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LandRegistry/land-registry-elements.git"
  },
  "author": {
    "name": "Andy Mantell",
    "email": "andy.mantell@digital.landregistry.gov.uk"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LandRegistry/land-registry-elements/issues"
  },
  "homepage": "https://github.com/LandRegistry/land-registry-elements#readme",
  "engines": {
    "npm": "3.x",
    "node": "5.x"
  },
  "devDependencies": {
    "htmllint": "^0.6.0",
    "node-resemble-js": "0.2.0",
    "pa11y": "^4.6.0",
    "sanitize-filename": "^1.6.0",
    "selenium-standalone": "^5.11.2",
    "should": "^11.2.0",
    "superagent": "^3.4.1",
    "supertest": "^3.0.0",
    "throat": "^3.0.0",
    "trim-character": "^2.0.1",
    "webdriverio": "^4.0.7",
    "webshot": "^0.18.0",
    "zip-dir": "^1.0.2"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "browserify": "^13.3.0",
    "browserify-incremental": "^3.1.1",
    "browsernizr": "^2.1.0",
    "clarify": "^2.0.0",
    "closest": "0.0.1",
    "compression": "^1.6.1",
    "conduct.js": "1.0.0",
    "copy": "^0.3.0",
    "delegate": "3.1.1",
    "dependency-graph": "^0.5.0",
    "domify": "1.4.0",
    "errorhandler": "^1.4.3",
    "es6-promise": "^4.0.5",
    "express": "^4.13.4",
    "express-tgz": "0.0.3",
    "extend": "^3.0.0",
    "glob": "^7.0.3",
    "govuk-elements-sass": "2.2.1",
    "govuk_template_mustache": "0.19.2",
    "govuk_frontend_toolkit": "5.0.3",
    "handlebars": "^4.0.5",
    "hogan.js": "3.0.2",
    "hoganify": "0.2.0",
    "js-htmlencode": "^0.2.0",
    "js-yaml": "^3.6.0",
    "leaflet": "^1.0.3",
    "lodash": "^4.12.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.4.0",
    "pkg-up": "^1.0.0",
    "postcss": "^5.0.21",
    "proj4leaflet": "^1.0.1",
    "promise-helpers": "^0.2.0",
    "pubsub-js": "1.5.4",
    "remarkable": "^1.6.2",
